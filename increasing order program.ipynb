    "def last(l):\n",
    "    return l[-1]\n",
    "def sort(given_list):\n",
    "    return sorted (given_list, key=last)\n",
    "\n",
    "given_list=[(2, 5), (1, 2), (4, 4), (2, 3), (2, 1)]\n",
    "print(\"sorted: \")\n",
    "print(sort(given_list))\n",
